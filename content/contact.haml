      %input#email.fancyinput
      %label{:for => "email"} Email
    .input
      %textarea#message.fancyinput
      %label{:for => "message"} Message
    .input 
      %input#robot.fancyinput
      %label{:for => "robot"} Are you a Robot? (yes/no)
    %input{:type => "submit"}
